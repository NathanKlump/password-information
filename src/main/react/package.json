{
  "name": "password-information-portlet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4",
    "rewire": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node scripts/build-non-split.js && npm run build:clean",
    "build:clean": "cd build/static/js/ && mv main.*.js main.js && cp main.*.js.map main.js.map && mv main.*.js.LICENSE.txt main.js.LICENSE.txt && cd ../css && mv main.*.css main.css && cp main.*.css.map main.css.map",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "npx prettier --write \"src/**/*.js\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
